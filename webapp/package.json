{
  "name": "front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --cache --quiet --ignore-path .gitignore .",
    "lint:js:fix": "eslint --ext \".js,.vue\" --fix --quiet --ignore-path .gitignore .",
    "lint:css": "stylelint --cache --quiet --ignore-path .gitignore **/*.{css,scss,sass}",
    "lint:css:fix": "stylelint --fix --quiet --ignore-path .gitignore **/*.{css,scss,sass}",
    "lint": "yarn lint:js & yarn lint:css",
    "lint:fix": "yarn lint:js:fix & yarn lint:css:fix",
    "format": "prettier  --check  \"**/*.{js,ts,vue}\"",
    "format:fix": "prettier  --write \"**/*.{js,ts,vue}\""
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/firebase": "^7.6.1",
    "@nuxtjs/google-adsense": "^1.4.0",
    "@nuxtjs/markdownit": "^2.0.0",
    "@types/sanitize-html": "^2.3.2",
    "axios": "^0.21.2",
    "core-js": "^3.9.1",
    "firebase": "^8.10.0",
    "gql-query-builder": "^3.5.5",
    "graphql": "^15.5.1",
    "graphql-request": "^3.5.0",
    "highlight.js": "^11.2.0",
    "markdown-it": "^12.0.6",
    "markdown-it-emoji": "^2.0.0",
    "nuxt": "^2.15.3",
    "nuxt-typed-vuex": "^0.2.0",
    "nuxt-webfontloader": "^1.1.0",
    "octicons-vue": "^1.0.10",
    "sanitize-html": "^2.5.1",
    "vue-infinite-loading": "^2.4.5",
    "vue-js-modal": "^2.0.0-rc.6",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.3",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/color-mode": "^2.1.1",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.7.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-scss": "^3.21.0"
  }
}